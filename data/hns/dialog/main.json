{
    "type": "minecraft:multi_action",
    "title": "Hide'n'Seek by giulgu17",
    "external_title": "Hide'n'Seek menu...",
    "pause": false,
    "can_close_with_escape": true,
    "columns": 1,
    "actions": [
        {
            "label": {
                "text": "Start game",
                "color": "green"
            },
            "width": 80,
            "action": {
                "type": "minecraft:run_command",
                "command": "function hns:start"
            }
        },
        {
            "label": {
                "text": "Settings..."
            },
            "width": 120,
            "action": {
                "type": "minecraft:run_command",
                "command": "function hns:settings_open"
            }
        },
        {
            "label": {
                "text": "Kit selection"
            },
            "width": 120,
            "action": {
                "type": "minecraft:show_dialog",
                "dialog": "hns:kits"
            }
        }
    ],
    "exit_action": {
        "label": {
            "text": "Exit menu"
        }
    }
}